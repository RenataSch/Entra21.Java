public class Data {
    
    private int dia;
    private int mes;
    private int ano;
    
    public void setDia (int dia) {
        this.dia = dia; // this.dia o dia declarao la em cima = dia que vou declarar no get.
    }
    public void setMes(int mes) {
        this.mes = mes;
    }
    public void setAno(int ano) {
        this.ano = ano;
    }
    public int getDia(){
        return this.dia;
    }
    public int getMes(){
        return this.mes;
    }
    public int getAno(){
        return this.ano;
    }
    public String toString(){
        return this.dia + "/" + this.mes + "/" + this.ano;
    }
}
